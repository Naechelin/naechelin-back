<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cf.naechelin.mapper.MemberTypeMapper">
    <select id="selectByMemberTypeMember" parameterType="MemberType">
        SELECT * FROM member_type ORDER BY member_type_name
    </select>
    <select id="insert" parameterType="MemberType">
        INSERT INTO member_type VALUES (#{memberType}, #{memberTypeName})
    </select>
    <update id="update" parameterType="MemberType">
        UPDATE member_type SET member_type_name=#{MemberTypeName}
        WHERE member_type=#{memberType}
    </update>
    <delete id="delete" parameterType="java.lang.String">
        DELETE FROM member_type WHERE member_type_name=${memberTypeName}
    </delete>
</mapper>
