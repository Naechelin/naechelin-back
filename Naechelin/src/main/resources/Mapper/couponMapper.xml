<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cf.naechelin.mapper.CouponMapper">
    <select id="list" parameterType="Query" resultType="Coupon">
        SELECT * FROM coupon;
    </select>

    <insert id="insert" parameterType="Line" useGeneratedKeys="true" keyProperty="lineId">
        INSERT INTO line VALUES(DEFAULT, #{memberId}, DEFAULT)
    </insert>
</mapper>
